          {
            "description": "Enable pretty-printing for gdb",
            "text": "-enable-pretty-printing",
            "ignoreFailures": true
          }
        ],
        "preLaunchTask": "build and run",
        "miDebuggerPath": "gdb",
        "logging": {
          "moduleLoad": false
        }
      }
    ]
  }